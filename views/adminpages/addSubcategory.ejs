<%- include('./header')%>

    <div class="page-wrapper" style="width: 130%;">
        <div class="formcontainer">
            <div class="row">
                <div class="col-md-6">
                    <div class="card"   style="margin: 0  0  0  -80px;">
                        <form class="form-horizontal" method="post" action="/admin/subcatogry/insertsubcatogrydata">
                            <div class="card-body" style="width: 100%;">
                                <h4 class="card-title">Category Info</h4>
                                <div class="form-group row">
                                    <label for="lname"
                                        class="col-sm-3 text-right control-label col-form-label">Select Category</label>
                                        <div class="col-12 col-md-9">
                                            <select name="catogry" id="select" class="form-control">
                                                <option value="">Select Category</option>
                                                <%for(var v of cate){%>
                                                    <option value="<%=v.id%>">
                                                        <%=v.category%>
                                                    </option>
                                                    <%}%>
                                            </select>
                                        </div>
                                </div>
                                <div class="form-group row">
                                    <label for="fname" class="col-sm-3 text-right control-label col-form-label">Add Subcategory</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control" id="fname" name="subcategory"
                                            placeholder="First Name Here">
                                    </div>
                                </div>
                              
                            <div class="border-top">
                                <div class="card-body">
                                    <button type="submit" class="btn btn-primary">Submit</button>
                                </div>
                            </div>
                        </form>
                    </div>          
                </div>
            </div>
            <!-- editor -->
        </div>
        <%- include('./footer') %>