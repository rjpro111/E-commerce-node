<%- include('./header')%>

    <div class="page-wrapper">
        <div class="formcontainer">
            <div class="row">
                <div class="col-md-6">
                    <div class="card">
                        <form class="form-horizontal" method="post" action="/admin/type/inserttypedata">
                            <div class="card-body" style="width: 100%;">
                                <h4 class="card-title">Category Info</h4>
                                <div class="form-group row">
                                    <label for="lname" class="col-sm-3 text-right control-label col-form-label">Select
                                        Category</label>
                                    <div class="col-12 col-md-9">
                                        <select name="catogry" id="product-catogry" class="form-control"
                                            onchange="return getsubcat()">
                                            <option value="">Select Category</option>
                                            <%for(var v of categoryData){%>
                                                <option value="<%=v.id%>">
                                                    <%=v.category%>
                                                </option>
                                                <%}%>
                                        </select>
                                    </div>

                                    <label for="lname" class="col-sm-3 text-right control-label col-form-label">Select
                                        SubCategory</label>
                                    <div class="col-12 col-md-9">
                                        <select name="subcategory" id="Subcatogry" class="form-control" onchange="return getextracat()">
                                            <option value="">Select SubCategory</option>
                                            <%for(var v of subcategoryData){%>
                                                <option value="<%=v.id%>">
                                                    <%=v.subcategory%>
                                                </option>
                                                <%}%>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label for="fname" class="col-sm-3 text-right control-label col-form-label">Add Extracategory</label>
                                    <div class="col-sm-9">
                                        <select name="extracategory" id="extracategory" class="form-control" onchange="getbrand()">
                                            <option value="">Select ExtraCategory</option>
                                            <%for(var v of extracategoryData){%>
                                                <option value="<%=v.id%>">
                                                    <%=v.subcategory%>
                                                </option>
                                                <%}%>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label for="fname" class="col-sm-3 text-right control-label col-form-label">Add
                                        Brand</label>
                                    <div class="col-sm-9">
                                        <select name="brand" id="brand" class="form-control">
                                            <option value="">Select ExtraCategory</option>
                                            <%for(var v of brandData){%>
                                                <option value="<%=v.id%>">
                                                    <%=v.brand%>
                                                </option>
                                                <%}%>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label for="type" class="col-sm-3 text-right control-label col-form-label">Add
                                        type</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control" name="type" placeholder="type">
                                    </div>
                                </div>
                                <div class="border-top">
                                    <div class="card-body">
                                        <button type="submit" class="btn btn-primary">Submit</button>
                                    </div>
                                </div>
                        </form>
                    </div>
                </div>
            </div>
            <!-- editor -->
        </div>
        <%- include('./footer') %>